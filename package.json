{
  "name": "zod-to-python",
  "version": "0.1.0",
  "description": "Convert Zod schemas to Python dataclasses",
  "type": "module",
  "bin": {
    "zod-to-python": "./dist/index.js"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:compile",
    "build:compile": "tsc",
    "build:clean": "rimraf ./dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webry-com/zod-to-python"
  },
  "keywords": [
    "zod",
    "python",
    "typescript",
    "dataclass",
    "types"
  ],
  "author": "Samuel Braun <sam@webry.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webry-com/zod-to-python/issues"
  },
  "homepage": "https://github.com/webry-com/zod-to-python#readme",
  "dependencies": {
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "ora": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.13",
    "@types/pkginfo": "^0.4.3",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
